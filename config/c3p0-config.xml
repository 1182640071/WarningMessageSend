<?xml version="1.0" encoding="UTF-8" ?>
<c3p0-config>
	<default-config>
		<property name="acquireIncrement">5</property>
		<property name="initialPoolSize">1</property>
		<property name="maxStatements">5</property>
		<property name="maxPoolSize">2</property>
		<property name="minPoolSize">1</property>
		<property name="jdbcUrl">
			jdbc:oracle:thin:@0.0.0.0:1521:gdqxt
		</property>
		<property name="driverClass">
			oracle.jdbc.driver.OracleDriver
		</property>
		<property name="user"></property>
		<property name="password"></property>
		<property name="idleConnectionTestPeriod">3600</property>
		<property name="maxIdleTime">600</property>
		<property name="numHelperThreads">5</property>
		<property name="acquireRetryAttempts">2</property>
		<property name="acquireRetryDelay">10</property>
		<property name="breakAfterAcquireFailure">false</property>
	</default-config>
</c3p0-config>
